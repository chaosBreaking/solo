    "query-string": "^6.9.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-filepond": "^7.0.1",
    "react-html-parser": "^2.0.2",
    "react-loadable": "^5.5.0",
    "react-toastify": "^6.2.0",
