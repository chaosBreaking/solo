{
  "name": "solo",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=8.16.2",
    "npm": ">=6.4.1"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "flow": "flow",
    "flow:check": "flow check",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "NODE_ENV=production babel-node tools/run build --release",
    "build-stats": "yarn run build --release --analyse",
    "render": "babel-node tools/run render",
    "serve": "NODE_ENV=production babel-node tools/run runServer",
    "dev": "cross-env NODE_ENV=development babel-node tools/run start",
    "debug": "babel-node --inspect-brk tools/run start",
    "deploy": "npm run build && pm2 start npm --name soloApp -- run serve"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.12.5",
    "@material-ui/core": "^4.11.2",
    "@sentry/browser": "^5.15.4",
    "@sentry/react": "^5.21.4",
    "@sentry/tracing": "^5.21.4",
    "@tinymce/tinymce-react": "^3.6.0",
    "axios": "^0.20.0",
    "better-react-spinkit": "^2.0.4",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "cos-js-sdk-v5": "^1.0.1",
    "express": "^4.16.3",
    "filepond": "^4.18.0",
    "filepond-plugin-file-validate-size": "^2.2.1",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "filepond-plugin-image-exif-orientation": "^1.0.9",
    "filepond-plugin-image-preview": "^4.6.4",
