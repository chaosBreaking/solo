    "happypack": "^5.0.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "log-symbols": "^3.0.0",
    "markdown-it": "^10.0.0",
    "mkdirp": "^0.5.1",
    "null-loader": "^3.0.0",
    "opn-cli": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pixrem": "^5.0.0",
    "pleeease-filters": "^4.0.0",
    "postcss": "^7.0.2",
    "postcss-calc": "^7.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
